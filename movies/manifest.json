{
    "name": "Movies Proxy [VOX]",
    "short_name": "Movies",
    "id": "evx-epsilon",
    "start_url": "/movies/",
    "background_color": "#000000",
    "display": "standalone",
    "tab_strip": {
      "new_tab_button": {
        "url": "/movies/"
      }
    },
    "orientation": "portrait",
    "dir": "ltr",
    "scope": "/movies/",
    "theme_color": "#000000",
    "lang": "en-US",
    "categories": [
      "entertainment",
      "lifestyle"
    ],
    "description": "Nexus: A versatile application serving as a central hub for accessing Evox apps like Tasco and Images, along with features for managing tasks, secure galleries, encrypted chats, and more. Your all-in-one solution for streamlined organization and secure communication.",
    "related_applications": [{
        "platform": "webapp",
        "url": "https://evoxs.xyz/movies/manifest.json"
      }
    ],
    "icons": [
      {
        "src": "movies.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "any"
      },
      {
        "src": "movies.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "maskable"
      },
      {
        "src": "movies.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
      },
      {
        "src": "movies.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
      }
    ],
    "share_target": {
      "action": "/share-target",
      "method": "GET",
      "enctype": "application/x-www-form-urlencoded",
      "params": {
        "title": "title",
        "text": "text",
        "url": "url"
      }
    },
    "edge_side_panel": {
      "preferred_width": 480
    },
    "handle_links": "auto",
    "launch_handler": {
      "client_mode": ["navigate-existing", "auto"]
    },
    "file_handlers": [
      {
        "action": "/file-handling",
        "accept": {
          "image/pgn": [".png"],
          "image/jpeg": [".jpg", ".jpeg"],
          "image/webp": [".webp"]
        },
        "icons": [
  
        ],
        "launch_type": "single-client"
      }
    ]
  }
  
{
  "name": "Evox Epsilon",
  "short_name": "Epsilon",
  "id": "evx-epsilon",
  "start_url": "/evox-epsilon/?source=pwa",
  "background_color": "#000",
  "display": "standalone",
  "display_override": ["tabbed"],
  "tab_strip": {
    "new_tab_button": {
      "url": "/evox-epsilon/"
    }
  },
  "orientation": "portrait",
  "dir": "ltr",
  "scope": "/evox-epsilon/",
  "theme_color": "#000",
  "lang": "en-US",
  "categories": [
    "entertainment",
    "lifestyle"
  ],
  "description": "Evox Gateway: A versatile application serving as a central hub for accessing Evox apps like Tasco and Images, along with features for managing tasks, secure galleries, encrypted chats, and more. Your all-in-one solution for streamlined organization and secure communication.",
  "related_applications": [{
      "platform": "webapp",
      "url": "https://evoxs.xyz/evox-epsilon/manifest.json"
    }
  ],
  "icons": [
    {
      "src": "epsilon-assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "epsilon-assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "epsilon-assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "epsilon-assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "edge_side_panel": {
    "preferred_width": 480
  },
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "file_handlers": [
    {
      "action": "/file-handling",
      "accept": {
        "image/pgn": [".png"],
        "image/jpeg": [".jpg", ".jpeg"],
        "image/webp": [".webp"]
      },
      "icons": [

      ],
      "launch_type": "single-client"
    }
  ]
}
